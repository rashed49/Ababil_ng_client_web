{
  "name": "ababil-ng-ui",
  "version": "4.2.0",
  "license": "COMMERCIAL",
  "angular-cli": {},
  "author": "Millennium Information Solution Ltd",
  "scripts": {
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "start": "ng serve --host 0.0.0.0 --port 4200 --disable-host-check",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "build": "npm run pre-build && ng build --prod --stats-json",
    "build-linux": "npm --max_old_space_size=2048 run pre-build && node --max_old_space_size=3072 $(which ng) build --prod --stats-json",
    "webdriver-update": "bash ./node_modules/.bin/webdriver-manager update",
    "test": "ng test --code-coverage --single-run",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "pre-build": "rimraf node_modules/@angular/cli/node_modules/uglify-es && rimraf node_modules/@angular/cli/node_modules/uglifyjs-webpack-plugin",
    "docker-build": "npm run pre-build && ng build",
    "dev-limbo": "ng serve --host 0.0.0.0 --port 4200 --disable-host-check --env=dev-limbo"
  },
  "dependencies": {
